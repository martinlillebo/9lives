
<div class="container bg-warning">
  <div class="row">
    <div class="col">
      <% @cats.each do |cat| %>
        <% if cat.user_id = current_user.id %>
          <b>Cats I Am Renting</b>
        <% end %>
      <% end %>
    </div>

    <div class="col bg-primary">
      <% @bookings.each do |booking| %>
        <% if booking.user_id = @user.id %>
          <b>Cats I have for rent</b>
        <% end %>
      <% end %>
    </div>
  </div>
  </div>
